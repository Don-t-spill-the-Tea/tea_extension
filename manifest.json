
{
  "name": "File Request Monitor",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Monitor Requests"
  }
}


